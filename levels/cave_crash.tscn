script/source = "extends Node2D
class_name cave_crash

@onready var door : Door = $door
@onready var cinematic : Area2D = $cinematic
@onready var block_boss : StaticBody2D = $block_boss
@onready var boss_cinematic : Node2D = $boss_cinematic
@onready var scientist_pod : ScientistPod = $scientist_pod

@onready var turret1 : Turret = $turret
@onready var turret2 : Turret = $turret2

@onready var door_turrets : Door = $door_turrets

func play_music(_file : String = \"\") -> void:
	var music_id : String = AudioPlayer.play(\"musics/Drifting Beyond the Stars.ogg\")
	AudioPlayer.set_bus_by_name(music_id,\"Music\")
	AudioPlayer.set_audio_process_mode(music_id, PROCESS_MODE_ALWAYS)
	AudioPlayer.add_callback(music_id,play_music)

func _ready() -> void:
	play_music()
	if !GS.data.get(\"intro\"):
		await Global.play_dialogue_player(
			\"intro_2.json\",
			{
				\"#{up}\":Global.get_actions_as_text(\"up\"),
				\"#{down}\":Global.get_actions_as_text(\"down\"),
				\"#{left}\":Global.get_actions_as_text(\"left\"),
				\"#{right}\":Global.get_actions_as_text(\"right\")
			}
		)
	else:
		door.open(false)
	# If boss started, remove the block blocking boss room
	if GS.data.get(\"boss_1_cave_crash\"):
		block_boss.queue_free()
		scientist_pod.queue_free()

func _on_save_pillar_interacted() -> void:
	if !GS.data.get(\"intro\"):
		door.open()
		GS.data[\"intro\"] = true
		GS.save.call_deferred(GS.auto_save_name)


func _on_scientist_pod_interacted() -> void:
	var boss_time : bool = false
	# Didn't talk to the scientist and got the dash
	if !GS.data.get(\"cave_crash_sc_dial1\") && GS.data.get(\"dash_aquired\"):
		GS.data[\"cave_crash_sc_dial1\"] = true
		boss_time = true
		await Global.play_dialogue_player(\"cave_crash_sc_3.json\",{},false)
	# Didn't talk to the scientist
	elif !GS.data.get(\"cave_crash_sc_dial1\"):
		await Global.play_dialogue_player(\"cave_crash_sc_1.json\",{
			\"#{shoot}\":Global.get_actions_as_text(\"left_click\"),
			\"#{autotarget}\":Global.get_actions_as_text(\"auto_target\")
		})
		GS.data[\"cave_crash_sc_dial1\"] = true
	# Talked to the scientist and do not have the dash
	elif !GS.data.get(\"dash_aquired\"):
		await Global.play_dialogue_player(\"cave_crash_sc_2.json\")
	# Talked to the scientist and got the dash
	elif GS.data.get(\"dash_aquired\") && !GS.data.get(\"boss_1_cave_crash\"):
		boss_time = true
		GS.data[\"boss_1_cave_crash\"] = true
		await Global.play_dialogue_player(\"cave_crash_sc_4.json\",{},false)
	if boss_time:
		GS.data[\"boss_1_cave_crash\"] = true
		block_boss.queue_free()
		var sound_id : String = AudioPlayer.play(\"sounds//Monster_SoftGrowl1.mp3\")
		AudioPlayer.set_bus_by_name(sound_id,\"VFX\")
		Global.shake_camera(Global.player.camera,Vector2(100,100),120,0.01)
		await get_tree().create_timer(2.4).timeout
		await Global.play_dialogue_player(\"cave_crash_sc_5.json\",{},false)
		Global.shake_camera(Global.player.camera,Vector2(100,100),60,0.01)
		boss_cinematic.play()
		


func _on_turret_died(_body : Variant = null) -> void:
	if (!turret1 || turret1.dying) && (!turret2 || turret2.dying):
		door_turrets.open()
"

