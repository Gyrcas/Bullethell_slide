script/source = "extends Interactable

@onready var sprite : AnimatedSprite2D = $sprite

func _ready() -> void:
	if GS.data.get(\"dash_aquired\"):
		queue_free()
		return
	sprite.play(\"default\",5)
	connect(\"body_entered\",_on_zone_body_entered)
	connect(\"body_exited\",_on_zone_body_exited)

func interact() -> void:
	await Global.play_dialogue_player(\"perk_wheel_tuto.json\",{
		\"#{perk}\":InputMap.action_get_events(\"perks\")[0].as_text()
	})
	GS.data[\"dash_aquired\"] = true
	Global.player.perks_wheel.perks.append(DashPerk.new())
	interacted.emit()
	queue_free()
"

