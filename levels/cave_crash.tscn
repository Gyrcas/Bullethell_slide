script/source = "extends Area2D

@export var zoom_cinematic : Vector2 = Vector2(0.15,0.15)
@export var tween_time : float = 3.0
@onready var target : Node2D = $target

var playing_cinematic : bool = false

func _ready() -> void:
	if GS.data.get(\"cine_cave_crash_intro\"):
		queue_free()

func _on_body_entered(body : Node2D) -> void:
	if not body is Player || playing_cinematic:
		return
	playing_cinematic = true
	body.controllable = false
	var old_zoom : Vector2 = body.camera.zoom
	var tween : Tween = create_tween()
	tween.parallel().tween_property(body.camera,\"zoom\",zoom_cinematic,tween_time).set_ease(Tween.EASE_OUT)
	tween.parallel().tween_property(body.camera,\"global_position\",target.global_position,tween_time).set_ease(Tween.EASE_OUT)
	tween.parallel().tween_property(body,\"velocity\",Vector2.ZERO,1).set_ease(Tween.EASE_OUT)
	await tween.finished
	await get_tree().create_timer(3).timeout
	tween = create_tween()
	tween.parallel().tween_property(body.camera,\"zoom\",old_zoom,tween_time / 2).set_ease(Tween.EASE_OUT)
	tween.parallel().tween_property(body.camera,\"position\",Vector2.ZERO,tween_time / 2).set_ease(Tween.EASE_OUT)
	await tween.finished
	body.controllable = true
	GS.data[\"cine_cave_crash_intro\"] = true
	queue_free()
"

