[sub_resource type="GDScript" id="GDScript_hq5b2"]
script/source = "extends Node2D
class_name boss_level_1

@onready var boss : Boss1 = $boss_1

var tween : Tween

var finished_intro : bool = false

func play_music(_file : String = \"\") -> void:
	var music_id : String = AudioPlayer.play(\"musics/Ove Melaa - Anno 2360.ogg\")
	AudioPlayer.set_volume(music_id,-10)
	AudioPlayer.set_bus_by_name(music_id,\"Music\")
	AudioPlayer.set_audio_process_mode(music_id, PROCESS_MODE_ALWAYS)
	AudioPlayer.add_callback(music_id,play_music)
	
func _ready() -> void:
	play_music()
	Global.player.controllable = false
	await get_tree().create_timer(1).timeout
	
	#Move camera to boss
	tween = create_tween()
	tween.tween_property(Global.player.camera,\"global_position\",boss.global_position,2.5)
	await tween.finished
	
	#Play sound and shake camera
	var sound_id : String = AudioPlayer.play(\"sounds/Monster_Roar15.mp3\")
	AudioPlayer.set_bus_by_name(sound_id,\"VFX\")
	Global.shake_camera(
		Global.player.camera,
		Vector2(100,100),
		125,
		0.01,
		Global.player.camera.position,
		func():
			await Global.play_dialogue_player(\"boss_dial.json\",{},false)
			#Move camera back to player after shake end
			tween = create_tween()
			tween.tween_property(Global.player.camera,\"position\",Vector2.ZERO,1)
			await tween.finished
			Global.player.controllable = true
			await get_tree().create_timer(4).timeout
			boss.activate()
	)

func _on_boss_1_died(_body : Variant = null) -> void:
	Engine.time_scale = 1
	Global.player.anim.play(\"end\")

func _on_player_end_anim_done() -> void:
	Global.change_scene_to_file(\"end_demo.tscn\")
"

