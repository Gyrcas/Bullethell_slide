[sub_resource type="GDScript" id="GDScript_o7n4e"]
script/source = "extends Node2D

var range_min : float = 3
var range_max : float = 4

func _ready() -> void:
	for child in get_children():
		var timer : Timer = Timer.new()
		(func():
			add_child(timer)
			var speed : float = randf_range(range_min,range_max)
			timer.start(speed)
			timer.connect(\"timeout\",on_timer_timeout.bind(child,timer,speed))
		).call_deferred()
		
		

func on_timer_timeout(laser : Laser, timer : Timer,speed : float) -> void:
	timer.start(speed)
	if laser.is_open:
		laser.close()
	else:
		laser.open()
"

